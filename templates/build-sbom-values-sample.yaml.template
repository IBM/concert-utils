concert:
  builds:
  - component_name: ${COMPONENT_NAME}
    output_file: "${COMPONENT_NAME}-build-inventory.json" # optional
    app_name: "${APP_NAME}"
    number: 1
    version: "${APP_VERSION}"
    repository:
      name: "${REPO_NAME}"
      url: "${REPO_URL}"
      branch: "${REPO_BRANCH}"
      commit_sha: "${REPO_COMMIT_SHA}"
    image:
      name: "${IMAGE_NAME}"
      tag: "${IMAGE_TAG}"
      digest: "${IMAGE_DIGEST}"